ifndef config
  config=debug
endif

ifndef verbose
  SILENT = @
endif

CC = gcc
CXX = g++
AR = ar

ifndef RESCOMP
  ifdef WINDRES
    RESCOMP = $(WINDRES)
  else
    RESCOMP = windres
  endif
endif

ifeq ($(config),debug)
  OBJDIR     = Debug
  TARGETDIR  = ..
  TARGET     = $(TARGETDIR)/cs488Project
  DEFINES   += -DDEBUG
  INCLUDES  += -I../../shared -I../../shared/include -I../../shared/gl3w -I../../shared/imgui -I../../shared/lodepng -I../../shared/irrKlang-1.6.0/include
  ALL_CPPFLAGS  += $(CPPFLAGS) -MMD -MP $(DEFINES) $(INCLUDES)
  ALL_CFLAGS    += $(CFLAGS) $(ALL_CPPFLAGS) $(ARCH) -g -std=c++11
  ALL_CXXFLAGS  += $(CXXFLAGS) $(ALL_CFLAGS)
  ALL_RESFLAGS  += $(RESFLAGS) $(DEFINES) $(INCLUDES)
  ALL_LDFLAGS   += $(LDFLAGS) -L../../lib -L. -framework IOKit -framework Cocoa -framework CoreVideo -framework OpenGL
  LDDEPS    +=
  LIBS      += $(LDDEPS) -lcs488-framework -limgui -lglfw3 -llodepng -lirrKlang
  LINKCMD    = $(CXX) -o $(TARGET) $(OBJECTS) $(RESOURCES) $(ARCH) $(ALL_LDFLAGS) $(LIBS)
  define PREBUILDCMDS
  endef
  define PRELINKCMDS
  endef
  define POSTBUILDCMDS
  endef
endif

ifeq ($(config),release)
  OBJDIR     = Release
